{
  "name": "codex-sessions",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 5172",
    "build": "vite build",
    "preview": "vite preview --host --port 5172",
    "docker:build": "docker build -t codex-sessions .",
    "docker:unix:run": "docker run --name codex-sessions --rm -p 9172:9172 -e VITE_PORT=9172 -v ~/.codex/sessions:/app/sessions -d codex-sessions"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "vuetify": "^3.7.2",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "vite": "^5.4.10",
    "vite-plugin-vuetify": "^2.0.4"
  }
}
